{
  "name": "n22-mobile-vue-mpaas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:codedata": "webpack --config build/build-code-data",
    "build:codetables": "node build/build-code-tables",
    "create": "node build/component-init-create.js",
    "init:module:default": "node build/init-module.js env=int select=default",
    "init:module:no": "node build/init-module.js env=int select=no",
    "init:module:yes": "node build/init-module.js env=local select=yes",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue modules",
    "lint:fix": "eslint --fix --ext .js,.vue modules",
    "shell": "npm run build:codedata && node build/init-module.js env=int select=yes && node build/shell/shell.js",
    "myserve": "node build/service/my-service.js",
    "precommit": "lint-staged",
    "dev": "node build/service/my-service.js serve --mode dev",
    "int": "npm run build:codedata && node build/service/my-service.js build --mode int",
    "uat": "npm run build:codedata && node build/service/my-service.js build --mode uat",
    "sta": "npm run build:codedata && node build/service/my-service.js build --mode sta",
    "prod": "npm run build:codedata && node build/service/my-service.js build --mode prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "modules/**/*.{vue,js,jsx,json,css,scss,md}": [
      "eslint --fix --ext .js,.vue",
      "prettier --write",
      "git commit"
    ]
  },
  "dependencies": {
    "al-mobile": "0.0.15",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "eruda": "^2.2.1",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "vue": "^2.6.11",
    "vue-content-loader": "^0.2.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.5",
    "vue-wechat-title": "^2.0.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "compression-webpack-plugin": "^3.1.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "git-user-email": "^0.2.2",
    "git-user-name": "^2.0.0",
    "handlebars": "^4.7.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "metalsmith": "^2.3.0",
    "mockjs": "^1.1.0",
    "postcss-aspect-ratio-mini": "^1.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-write-svg": "^3.0.1",
    "prettier": "^1.19.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^4.2.1",
    "terser-webpack-plugin": "^2.3.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpackbar": "^4.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
